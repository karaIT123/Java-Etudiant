<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.etudiant">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".etudiant_activite"></activity>
        <activity android:name=".modifier_compte" />
        <activity android:name=".voir_activite" />
        <activity android:name=".statistique" />
        <activity android:name=".proposer_activite" />
        <activity android:name=".noter_activite" />
        <activity android:name=".note_moyenne" />
        <activity android:name=".liste_inscription" />
        <activity android:name=".inscrire_activite" />
        <activity android:name=".creer_compte" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>